{
    "name": "zenmail/zen-swift-adapter-bundle",
    "type": "symfony-bundle",
    "description": "Zen Swift Adapter Bundle, part and core of ZenMail Project",
    "keywords": ["swiftmailer", "email", "mail"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alberto Vioque",
            "email": "mashware@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/event-dispatcher": ">=2.2",
        "symfony/config": ">=2.2",
        "symfony/http-kernel": ">=2.2",
        "symfony/dependency-injection": ">=2.2",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "target-dir": "ZenMail/ZenSwiftAdapterBundle",
    "autoload": {
        "psr-0": { "ZenMail\\ZenSwiftAdapterBundle": "" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
