{
    "name": "zenmail/zen-swift-adapter-bundle",
    "type": "symfony-bundle",
    "description": "ZenMail, admin send email",
    "keywords": ["Send email", "email", "mail"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alberto Vioque",
            "email": "mashware@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",

        "symfony/http-kernel": "~2.6",
        "symfony/config": "~2.6",
        "symfony/yaml": "~2.6",
        "symfony/console": "~2.6",
        "symfony/framework-bundle": "~2.6",
        "symfony/class-loader": "~2.6",
        "symfony/security": "~2.6",
        "symfony/http-foundation": "~2.6",
        "symfony/validator": "~2.6",
        "symfony/process": "~2.6",
        "symfony/event-dispatcher": "~2.6",
        "symfony/expression-language": "~2.6",
        "symfony/finder": "~2.6",
        "symfony/browser-kit": "~2.6",
        "symfony/routing": "~2.6",
        "symfony/proxy-manager-bridge": "~2.6, >=2.6.3",
        "symfony/twig-bundle": "~2.6",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/form": "~2.6",

        "doctrine/common": "~2.4",
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-cache-bundle": "~1.0"

    },
    "replace": {
        "zenmail/zen-core-bundle": "self.version",
        "zenmail/zen-swift-adapter-bundle": "self.version"
    },

    "autoload": {
        "psr-4": {
            "ZenMail\\": "src/ZenMail"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
